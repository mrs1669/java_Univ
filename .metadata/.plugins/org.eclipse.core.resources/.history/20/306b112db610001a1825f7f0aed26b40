import java.util.*;


public class Manager {
	
	private ArrayList<Card> cards = new ArrayList<Card>();
	
	Manager(Card cards){
		this.cards = new ArrayList<Card>();
	}

	public void addMember(String name) {
		cards.add(new Card(name, 1));
	}

